{
  "name": "formater",
  "version": "1.0.0",
  "description": "Trading data formatter",
  "keywords": [
    "portal",
    "formatter",
    "linter"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "big.js": "^7.0.1",
    "bybit-api": "^4.1.7",
    "commander": "^13.1.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "sinon": "^20.0.0",
    "winston": "^3.17.0"
  },
  "type": "module",
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-ascii": "^2.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "mocha": "^11.2.2",
    "prettier": "^3.5.3",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "format": "yarn prettier --write .",
    "format:dry": "yarn prettier --check .",
    "lint": "yarn eslint .",
    "fix": "yarn eslint --fix .",
    "test": "mocha '**/*.test.mjs'",
    "test:coverage": "c8 npm test",
    "bybit": "node bybit.mjs",
    "bybit:debug": "node --inspect-brk=8000 src/bybit/execute.mjs"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "yarn fix",
      "yarn format"
    ],
    "*.{json,md,yaml,yml}": [
      "yarn format"
    ]
  }
}
