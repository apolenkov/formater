{
  "name": "formater",
  "version": "1.0.0",
  "description": "Format fund markets",
  "keywords": [
    "portal",
    "formatter",
    "linter"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "big.js": "^7.0.1",
    "bybit-api": "^4.1.5",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "type": "module",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ascii": "^2.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.6",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "format": "yarn prettier --write .",
    "format:dry": "yarn prettier --check .",
    "lint": "yarn eslint .",
    "fix": "yarn eslint --fix .",
    "prepare": "husky install"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "yarn fix",
      "yarn format"
    ],
    "*.{json,md,yaml,yml}": [
      "yarn format"
    ]
  }
}
